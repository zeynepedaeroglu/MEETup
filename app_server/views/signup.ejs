<title>SignUp</title>
<div class="container">
    <div class="row">
        <div class="LoginPanel">
            <div class="panel panel-info">
                <div class="panel-heading">
                    <div class="panel-title">Kayıt Paneli</div>
                </div>
                <div class="panel-body">
                    <form action="/login/signup" method="POST">
                        <!--ad-->
                        <div style="margin-bottom: 25px" class="input-group">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                            <input id="login-username" type="text" class="form-control" name="ad" placeholder="İsim">
                        </div>
                        <!--soyad-->
                        <div style="margin-bottom: 25px" class="input-group">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                            <input id="login-username" type="text" class="form-control" name="soyad" placeholder="Soyisim">
                        </div>
                        <!--email-->
                        <div style="margin-bottom: 25px" class="input-group">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                            <input id="login-username" type="text" class="form-control" name="email" placeholder="E-Mail">
                        </div>
                        <!--kullanıcı adı-->
                        <div style="margin-bottom: 25px" class="input-group">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
                            <input id="login-username" type="text" class="form-control" name="kullaniciAdi" placeholder="Kullanıcı Adı">
                        </div>
                        <!--şifre-->
                        <div style="margin-bottom: 25px" class="input-group">
                            <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
                            <input id="password" type="password" class="form-control" name="sifre" placeholder="Şifre">
                        </div>
                        <br/>
                        <input type="submit" class="btn btn-primary" value="Kaydet">
                        <a href="/login" class="btn btn-default">Giriş Yap</a>
                        <%console.log('signUp sayfası açıldı.')%>
                    </form>
                </div>
                <div class="panel-footer clearfix">
                    <div class="pull-right">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>